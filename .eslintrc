{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier", "prettier/flowtype"],
  "plugins": ["flowtype", "import"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off"
  }
}
